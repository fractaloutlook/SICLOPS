# SICLOPS Environment Configuration

# Required: Anthropic API Key for Claude models
ANTHROPIC_API_KEY=your_anthropic_key_here

# Optional: OpenAI API Key for GPT models
OPENAI_API_KEY=your_openai_key_here

# Optional: Show full prompts sent to agents (for debugging)
# Values: true | false
# Default: false
SHOW_AGENT_PROMPTS=false

# Optional: Verbose SharedMemoryCache logging
# Values: true | false
# Default: false
VERBOSE_CACHE_LOGGING=false
